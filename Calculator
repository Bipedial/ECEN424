package mycalc;

//Not sure why github won't let me put a description without editting the code
import java.io.IOException;

/**
 * Created by Cemre Tas on 9/22/2015.
 */
public class Calculator {

        private String name;
        public float addition(Float A, Float B){
            float C;
            C = A + B;
            return C;
        }
        public float subtraction(Float A, Float B){
            float C;
            C = A - B;
            return C;
        }
        public float multiplication(Float A, Float B){
            float C;
            C = A * B;
            return C;
        }
       public void setname(String N){
         name = N;
        }
        public String getname(){

            return name;
        }

}
